     "output_type": "execute_result"
    }
   ],
   "source": [
    "from scipy.stats import uniform\n",
    "\n",
    "# Probabilidade de ao lançar um dado de 6 faces obter uma face qualquer\n",
    "round(uniform.cdf(1,0,6),4)"
