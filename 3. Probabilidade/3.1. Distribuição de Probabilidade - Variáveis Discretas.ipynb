   ],
   "source": [
    "from scipy.stats import hypergeom\n",
    "\n",
    "N = 52  # tamanho da população\n",
    "K = 4   # número de sucessos na população\n",
    "n = 5  # retiradas\n",
    "x = 1\n",
    "\n",
    "round(hypergeom.pmf(x, N, K, n),4)"
